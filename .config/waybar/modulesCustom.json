{
    "custom/separator#dot-line": {
        "format": "î˜¡",
        "interval": "once",
        "tooltip": false
    },
    "custom/separator#line": {
        "format": "|",
        "interval": "once",
        "tooltip": false
    },
    "custom/github": {
        "format": "{}",
        "return-type": "json",
        "interval": 1200,
        "exec": "~/.config/waybar/scripts/github.sh",
        "on-click": "xdg-open https://github.com/MolyiEZ",
        "on-click-right": "pkill -SIGRTMIN+8 waybar",
        "tooltip": true,
        "signal": 8
    },
    "custom/monkeytype": {
        "format": "{}",
        "return-type": "json",
        "interval": 1200,
        "exec": "~/.config/waybar/scripts/monkeytype.sh",
        "on-click": "xdg-open https://monkeytype.com/profile/Molyi",
        "on-click-right": "pkill -SIGRTMIN+8 waybar",
        "tooltip": true,
        "signal": 8
    },
    "custom/leetcode": {
        "format": "{}",
        "return-type": "json",
        "interval": 1200,
        "exec": "~/.config/waybar/scripts/leetcode.sh",
        "on-click": "xdg-open https://leetcode.com/u/Molyi/",
        "on-click-right": "pkill -SIGRTMIN+8 waybar",
        "tooltip": true,
        "signal": 8
    }
}
